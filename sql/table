CREATE TABLE heroes(
  id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
  name VARCHAR(255),
  real_name VARCHAR(255),
  location VARCHAR(255),
  is_real_hero VARCHAR(3) DEFAULT 'Yes',
  state VARCHAR(4) DEFAULT 'GOOD',
  score INT DEFAULT 0,
  thumb TEXT
);

